<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<context:annotation-config />
	
	<bean id = "address" class="com.vidvaan.bean.Address">
	<property name="aid" value = "1"></property>
	<property name="aloc" value = "HYD"></property>
	</bean>
	<bean id="employee" class="com.vidvaan.bean.Employee">
	<property name="eid" value = "101"></property>
	<property name="ename" value = "Satya"></property>
	<property name="address" ref="address"></property>
	
	</bean>
	</beans>